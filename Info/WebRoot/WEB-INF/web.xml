<?xml version="1.0" encoding="UTF-8"?>
<web-app id="Info" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>Info</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>list.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>ServletConfigurator</servlet-name>
		<servlet-class>
			org.logicalcobwebs.proxool.configuration.ServletConfigurator
		</servlet-class>
		<init-param>
			<param-name>xmlFile</param-name>
			<param-value>
				WEB-INF/classes/config/xp_proxool.xml
			</param-value>
		</init-param>
		<load-on-startup>2</load-on-startup>
	</servlet>
	
	<servlet>
		<servlet-name>Log4jInitServlet</servlet-name>
		<servlet-class>
			log4j.Log4jInitServlet
		</servlet-class>
		<init-param>
			<param-name>log4j.init.filename</param-name>
			<param-value>
				WEB-INF/classes/config/log4j.properties
			</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	
	<servlet>
		<servlet-name>triggerInitServlet</servlet-name>
		<servlet-class>quartz.Trigger</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>
	
	
</web-app>